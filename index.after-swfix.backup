<!doctype html>
<html lang="fr">
  <head>
  <!-- SW unregister + cache purge + override fond -->
  <script>
  (function(){
    try {
      if ('serviceWorker' in navigator) {
        navigator.serviceWorker.getRegistrations().then(function(regs){
          regs.forEach(function(r){ r.unregister(); });
        });
      }
      if (window.caches && caches.keys) {
        caches.keys().then(function(keys){ keys.forEach(function(k){ caches.delete(k); }); });
      }
      // Eviter boucle de reload
      if (!sessionStorage.getItem('sw_cleared')) {
        sessionStorage.setItem('sw_cleared','1');
        // Donne 150ms au navigateur pour purger puis recharge
        setTimeout(function(){ location.reload(); }, 150);
      }
    } catch(e) { console && console.warn && console.warn('SW clear failed', e); }
  })();
  </script>
  <style>
    html,body,#root{ background:#fff !important; color:#111 !important; }
  </style>
    <meta charset="UTF-8" />
    <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>EGOEJO</title>
  </head>
  <body>
    <div id="root"></div>
    <script type="module" src="/src/main.jsx"></script>
  </body>
</html>


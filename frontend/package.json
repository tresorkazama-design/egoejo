{
  "name": "frontend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "vite",
    "lint": "eslint src --ext .js,.jsx --max-warnings 0",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:coverage:threshold": "vitest run --coverage --coverage.threshold.lines=80 --coverage.threshold.functions=80 --coverage.threshold.branches=80 --coverage.threshold.statements=80",
    "test:run": "vitest run",
    "test:a11y": "vitest run src/test/accessibility.test.jsx",
    "test:integration": "BACKEND_AVAILABLE=true vitest run src/utils/__tests__/integration-backend.test.js",
    "test:backend": "vitest run src/utils/__tests__/backend-connection.test.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:backend": "playwright test e2e/backend-connection.spec.js",
    "test:e2e:production": "node scripts/test-e2e-production.js",
    "test:e2e:audit": "playwright test e2e/audit-compliance-accueil-vision.spec.js e2e/home.spec.js e2e/navigation-sections.spec.js",
    "audit:home-vision": "node scripts/audit-home-vision.mjs",
    "audit:home-vision:badge": "node scripts/copy-compliance-badge.js",
    "audit:global": "node scripts/audit-global.mjs",
    "test:performance": "node scripts/test-performance.js",
    "test:security": "npm audit --audit-level=moderate",
    "test:lighthouse": "node scripts/lighthouse-ci.js",
    "analyze": "vite build --mode analyze",
    "build:analyze": "vite build && npm run test:performance",
    "type-check": "tsc --noEmit || echo 'TypeScript not configured'",
    "prepare": "husky",
    "lint-staged": "lint-staged"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.1.7",
    "jest-axe": "^10.0.0",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.0",
    "msw": "^2.12.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.15",
    "vite": "^7.1.11",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^4.0.15"
  },
  "dependencies": {
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.4.0",
    "@tanstack/react-query": "^5.90.15",
    "decimal.js": "^10.6.0",
    "framer-motion": "^12.23.26",
    "gsap": "^3.13.0",
    "qrcode.react": "^4.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.4",
    "recharts": "^3.5.1",
    "three": "^0.180.0"
  }
}
